# Security test .env file - contains various potential security issues

# Sensitive values that should be masked
API_KEY=secret-api-key-456
TOKEN=bearer-token-789
SECRET=my-secret-value
PASSWORD=test-password-123
PASS=another-password

# Command injection attempts (should be blocked)
DANGEROUS_CMD=$(cat /etc/passwd)
COMMAND_INJECTION=`whoami`
PIPE_INJECTION=value|cat
SEMICOLON_INJECTION=value;ls
AMPERSAND_INJECTION=value&echo

# Backtick injection
BACKTICK_VALUE=`rm -rf /`

# Valid but suspicious looking values
SAFE_DOLLAR_VAR=$HOME/safe/path
SAFE_PARENTHESES=(valid parentheses content)

# Normal values mixed in
NORMAL_VAR=completely_safe_value
ANOTHER_NORMAL=123456